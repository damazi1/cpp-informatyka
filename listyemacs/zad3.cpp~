#include<iostram>

using namespace std;

void wypelnij(int *tab,const unsigned int n){
  tab[0]=0;
  tab[1]=1;
  for(int i=2;i<n;i++){
    tab[i]=tab[i-1]+tab[i-2];
  }
}

void wypisz(int *tab,const unsigned int n){
  int i=0;
  for(;i<n;i++){
    cout<<i+1<<tab[i]<<endl;
  }
}

void usun(int*tab){
  if(tab){
    delete [] tab;
    tab=nullptr;
  }
}

int main(int argc,char *argv[]){
  if (argc!=1){
    cerr<<"Podana liczba argumentow jest bledna(1)\n";
    cout<<"Poprawny zapis to ./nazwa\n";
    return 1;
  }
  int *tab;
  unsigned int n;
  cout<<"Podaj wielkosc tablicy: ";
  cin>>n;
  tab=new int [n];

  wypelnij(tab,n);
  wypisz(tab,n);

  usun(tab);

  return 0;
}
